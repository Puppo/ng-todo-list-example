{
  "name": "ng-todo-list-example",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "precommit": "npm run lint && npm run test && npm run build",
    "ng": "ng",
    "start:api": "node ./db/server.js",
    "prestart:api:test": "npm run dbtest:rest",
    "start:api:test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && node ./db/server.js ",
    "poststart:api:test": "npm run dbtest:rest",
    "start:ui": "ng serve",
    "start": "npm run start:api && npm run start:ui",
    "prestart:pwa": "npm run build",
    "start:pwa": "http-server dist",
    "build": "ng build --prod",
    "test": "ng test --watch=false --code-coverage --browser PhantomJS --reporters spec",
    "test:watch": "ng test --code-coverage --browser PhantomJS --reporters spec",
    "test:chrome": "ng test --code-coverage --browser Chrome",
    "lint": "ng lint",
    "e2e": "ng e2e --env=e2e",
    "dbtest:rest": "cp ./db/db-init/db-test.json ./db/db-test.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@angular/service-worker": "^8.2.5",
    "@ngrx/effects": "^8.3.0",
    "@ngrx/router-store": "^8.3.0",
    "@ngrx/store": "^8.3.0",
    "@ngrx/store-devtools": "^8.3.0",
    "core-js": "^3.2.1",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "moment": "^2.20.1",
    "ngrx-store-localstorage": "^8.0.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "^8.2.5",
    "@angular/language-service": "^8.2.5",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~12.7.4",
    "codelyzer": "^5.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.15.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "phantomjs-prebuilt": "^2.1.16",
    "prettier": "^1.10.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "typescript": "^3.5.3"
  }
